{
    "include": ["./src", "./proto-gen"],
    "compilerOptions": {
        "target": "ES2022",
        "module": "CommonJS",
        "lib": ["ES2022"],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strictNullChecks": true,
        "strict": true,
        "skipLibCheck": true
    }
}
